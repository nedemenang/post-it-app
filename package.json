{
  "name": "post-it-app",
  "version": "1.0.0",
  "description": "Post It App",
  "main": "index.js",
  "scripts": {
    "dev": "babel-watch server.js",
    "startdev": "NODE_ENV=development npm run build-dev && babel-node ./server/server.js",
    "start": "NODE_ENV=production node ./dist/server/server.js",
    "build-client": "webpack --config=webpack.config.prod.js",
    "build-dev": "webpack --config=webpack.config.js",
    "build": "rimraf /dist && concurrently \" npm run build-client\" \"npm run build-server\"",
    "build-server": "babel ./server --out-dir ./dist/server --copy-files",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov --token=b9101838-51a4-4e9d-bf62-c8b3ec65eb0b",
    "testServer": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --compilers js:babel-core/register --timeout 10000 ./server/test/*.test.js && npm run coverage",
    "testClient": "NODE_ENV=test jest --u ./client/__tests__/ --coverage && npm run coverage",
    "postinstall": "[\"$NODE_ENV\" != production] && exit 0; npm run build",
    "test": "npm run testServer && npm run testClient"
  },
  "author": "Nnamso Edemenang",
  "license": "ISC",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/client/jest/styleMock.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/client/jest/fileMock.js"
    }
  },
